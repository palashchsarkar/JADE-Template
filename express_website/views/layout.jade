doctype html
html
  head
    title #{title} : My Jade Template
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(rel='stylesheet', href='/stylesheets/bootstrap.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
  //- HEADER INCLUDED HERE ...
  include header

  nav.navbar.navbar-inverse.navbar-fixed-top
      .container
        .consolebutt
          a.btn.btn-primary Consol
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          // a.navbar-brand(href='/') DATALAKE
        #navbar.collapse.navbar-collapse
          ul.nav.navbar-nav
            li(class=(title=='Home'?'active':undefined))
              a(href='/') Home
            li(class=(title=='About'?'active':undefined))
              a(href='/about') About Us
            li(class=(title=='Contact'?'active':undefined))
              a(href='/contact') Contact Us

        // /.nav-collapse

    block content

  //- FOOTER INCLUDED HERE ...
  .contentarea
    #dlfooter.clearfix
      - footerlinks = ['Explore', 'Target', 'Source', 'Account Management', 'Help']
        ul.footerlinks
          li: a(href='#') #{footerlinks [0]}
          li: a(href='#') #{footerlinks [1]}
          li: a(href='#') #{footerlinks [2]}
          li: a(href='#') #{footerlinks [3]}
          li: a(href='#') #{footerlinks [4]}
        .footerlogo
          img(src='images/dlfooter_logo.png', alt='johnson & johnson')

script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js')
script(src='javascripts/bootstrap.js')
script(src='javascripts/swipe.js')
script(src='javascripts/custom.js')